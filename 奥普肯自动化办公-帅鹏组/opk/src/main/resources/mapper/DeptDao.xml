<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.dao.DeptDao">
	<select id="findDeptByDid" resultType="com.company.pojo.Dept">
		select * from dept where
		dId=#{dId}
	</select>
	
	<select id="findAll" resultType="com.company.pojo.Dept">
		select * from dept
	</select>

</mapper>