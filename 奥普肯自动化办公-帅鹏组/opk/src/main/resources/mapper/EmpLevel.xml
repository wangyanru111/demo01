<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.dao.EmpLevelDao">
	<select id="findELAll" resultType="com.company.pojo.EmpLevel">
		select * from emplevel

	</select>
	<select id="findELBylId" resultType="com.company.pojo.EmpLevel">
		select * from emplevel where
		lId=#{lId}

	</select>

</mapper>