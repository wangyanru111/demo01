<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.dao.ProjectTypeDao">
	<select id="findProTypeAll" resultType="com.company.pojo.ProjectType">
		select * from projecttype
	</select>
	<select id="findProTypeByPTId" resultType="com.company.pojo.ProjectType">
		select * from projecttype where proTid=#{proTid}
	</select>
</mapper>