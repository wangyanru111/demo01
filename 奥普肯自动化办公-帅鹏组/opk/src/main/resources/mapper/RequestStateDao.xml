<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.dao.RequestStateDao">
<select id="findStateAll" resultType="com.company.pojo.RequestState">
		select * from requeststate 
		
	</select>
	<select id="findStateBySId" resultType="com.company.pojo.RequestState">
		select * from requeststate where
		sId=#{sId} 
	</select>
</mapper>